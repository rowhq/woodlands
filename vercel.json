{
  "buildCommand": "npx expo export -p web",
  "outputDirectory": "dist",
  "devCommand": "npx expo start --web",
  "installCommand": "npm install",
  "framework": null,
  "public": true,
  "crons": [
    {
      "path": "/api/cron/refresh-events",
      "schedule": "*/5 * * * *"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}